
@{
    Layout = null;
}

@await Component.InvokeAsync("_PartialHeadAdmin")

<html>
<body>
    <div class="loader"></div>
    <div id="app">
        <div class="main-wrapper main-wrapper-1">
            @await Component.InvokeAsync("_PartialNavbarAdmin")

            @await Component.InvokeAsync("_PartialSidebarAdmin")
           
            <!-- Main Content -->
            <div class="main-content">
                @RenderBody()

                @await Component.InvokeAsync("_PartialSettingSidebar")
            
            </div>
            <footer class="main-footer">
                <div class="footer-left">
                    <a href="#"></a>
                </div>
                <div class="footer-right">
                </div>
            </footer>
        </div>
    </div>
    <script src="~/admin/assets/js/app.min.js"></script>
    <script src="~/admin/assets/js/scripts.js"></script>
    <script src="~/admin/assets/js/custom.js"></script>
</body>
</html>