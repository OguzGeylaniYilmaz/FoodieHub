@model CreateFeatureDto
@{
    ViewData["Title"] = "CreateFeature";
    Layout = "~/Views/Admin/Index.cshtml";
}


<section class="section">
    <div class="section-body">
        <div class="row">
            <div class="col-12 col-md-6 col-lg-12">
                <div class="card">
                    <div class="card-header">
                        <h4>Create Feature</h4>
                    </div>
                    <div class="card-body">
                        <form method="post">
                            <label>Title</label>
                            <input type="text" asp-for="Title" class="form-control" />
                            <br />
                            <label>Subtitle</label>
                            <input type="text" asp-for="SubTitle" class="form-control" />
                            <br />
                            <label>Description</label>
                            <input type="text" asp-for="Description" class="form-control" />
                            <br />
                            <label>Video Url</label>
                            <input type="text" asp-for="VideoUrl" class="form-control" />
                            <br />
                            <label>Image Url</label>
                            <input type="text" asp-for="ImageUrl" class="form-control" />
                            <br />
                            <button type="submit" class="btn btn-primary">Create</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>



